<!DOCTYPE html>
<html lang="en">
<head>
    <%- include header.ejs %>
</head>

<body>

<!-- Wrap all page content here -->
<div id="wrap">

    <!-- Fixed navbar -->
    <%- include navbar.ejs %>

    <!-- Begin page content -->
    <div class="container">
        <div class="page-header">
            You may click on any of the modules below to get you on the page with list of operations specific to it.
        </div>
        <div class="bs-example">
            <table class="table">
                <thead>
                <tr>
                    <th>Module</th>
                    <th>Description</th>
                </tr>
                </thead>
                <tbody>
                <%
                data.modules.forEach(function(mod,i){
                %>
                <tr>
                    <td><a href="<%=mod.name%>" class=""><%=mod.name%></a></td>
                    <td><%=mod.config.description%></td>
                </tr>
                <% }); %>
                </tbody>
            </table>
        </div>
        <!--<p>Back to main <a href="/">documentation</a> page.</p>-->

    </div>
</div>
<%- include footer.ejs %>
</body>
</html>
